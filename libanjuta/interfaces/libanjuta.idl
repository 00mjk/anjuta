	/* Types */
	enum Error
	{
		OK              =  0,
		NOT_READY       = -1,
		NOT_RUNNING     = -2,
		NOT_STOPPED     = -3,
		NOT_LOADED      = -4,
		NOT_STARTED     = -5,
		NOT_CONNECTED   = -6,
		NOT_IMPLEMENTED = -7
	}

	enum Data
	{
		INFORMATION,
		BREAKPOINT,
		FRAME,
		VARIABLE,
		REGISTER
	}

	enum OutputType
	{
		OUTPUT,
		WARNING_OUTPUT,
		ERROR_OUTPUT,
		INFO_OUTPUT
	}

	enum Boolean
	{
		UNDEFINED = 0,
		DEFINED = 1,
		FALSE = 1,
		BOOLEAN = 2,
		TRUE = 3
	}	

	enum Status
	{
		BUSY,
		STOPPED,
		STARTED,
		PROGRAM_LOADED,
		PROGRAM_RUNNING,
		PROGRAM_STOPPED
	}

	enum BreakpointType
	{
		BREAK_ON_LINE,
		BREAK_ON_ADDRESS,
		BREAK_ON_FUNCTION
	}

	struct Breakpoint
	{
		BreakpointType type;
		guint id;
		const gchar *file;
		guint line;
		const gchar *function;
		guint address;
		Boolean enable;
		guint ignore;
		guint times;
		const gchar *condition;
		gboolean keep;
	}

	struct Register
	{
		guint num;
		const gchar *name;
		const gchar *value;
	}

	struct Frame
	{
		guint level;
		const gchar *args;
		const gchar *file;
		guint line;
		const gchar *function;
		guint address;
	}

