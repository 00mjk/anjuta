[[AC_CONFIG_FILES(Makefile)
]])
AT_DATA([empty/Makefile.am],
[[
]])
AT_DATA([expect],
[[PROPERTY (Name): empty
    GROUP (0): empty
]])
AT_PARSER_CHECK([load empty \
		 set : name empty \
		 list \
		 save])
AT_CHECK([diff -b output expect])
AT_PARSER_CHECK([load empty \
		list])
AT_CHECK([diff -b output expect])



