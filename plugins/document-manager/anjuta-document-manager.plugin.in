[Anjuta Plugin]
_Name=Document Manager
_Description=Provides document management capabilities.
Location=anjuta-document-manager:DocmanPlugin
Version=0.1
Icon=anjuta-document-manager.png
Interfaces=IAnjutaDocumentManager,IAnjutaFile,IAnjutaFileSavable
UserActivatable=no

[File Loader]
SupportedMimeTypes=text/*,text/source,application/x-perl,application/x-python,application/x-shellscript,application/x-php,application/x-java,application/x-javascript,application/x-ruby,application/xml,application/x-m4
