				<menuitem name="Save" action="ActionFileSave" />
				<menuitem name="SaveAs" action="ActionFileSaveAs" />
				<menuitem name="SaveAll" action="ActionFileSaveAll" />
				<menuitem name="Close" action="ActionFileClose" />
