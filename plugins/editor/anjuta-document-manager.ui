					<menuitem name="FindInFiles" action="ActionEditSearchInFiles"/>
					<menuitem name="ISearch" action="ActionEditSearchSelectionISearch"/>
				</menu>
			</placeholder>
			<menu name="Goto" action="ActionMenuEditGoto">
